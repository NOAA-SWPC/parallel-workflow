    849 !=============================================================================
    850 function fLdLb_d(m,mah,a,ff)result(b)!                                   [LdLb]
    851 !=============================================================================
    852 use pietc, only: T,F
    853 use pmat2, only: clipb
    854 integer,                       intent(IN ):: m, mah
    855 real(dp),dimension(m,-mah:mah),intent(IN ):: a
    856 logical,                       intent(out):: ff ! <- failure flag
    857 real(dp),dimension(m,-mah:0)              :: b
    858 integer                                   :: i, j,k,jmi,lj,li
    859 real(dp)                                  :: s,te
    860 !=============================================================================
    861 ff=F
    862 call clipb(m,m,mah,0,b); b(:,0)=1
    863 do j=1,m; lj=max(-mah,1-j)
    864    s=a(j,0)
    865    do k=lj,-1
    866       s=s-b(j,k)**2*b(k+j,0)
    867    enddo
    868    if(s <= 0)then
    869       ff=T
    870       write(41,'(" In fLDLB_d; non-positivity at diagonal index",i5)'),j
    871       return
    872    endif
    873    b(j,0)=s; s=1/s
    874    do i=j+1,min(m,j+mah); jmi=j-i  
    875       li=max(-mah,1-i); 
    876       lj=li-jmi; 
    877       te=a(i,jmi)
    878       do k=li,jmi-1
    879          te=te-b(i,k)*b(j,k-jmi)*b(i+k,0)
    880       enddo
    881       b(i,jmi)=s*te
    882    enddo
    883 enddo
    884 b(:,0)=1/b(:,0)
    885 end function fLdLb_d

