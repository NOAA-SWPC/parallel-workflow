    802       ff=T
    803       write(41,'("In fL1LB_d; non-positivity at diagonal index",i5)'),j
    804       return
    805    endif
    806    s=sqrt(s); b(j,0)=s; s=1/s
    807    do i=j+1,min(m,j+mah); jmi=j-i
    808       b(i,jmi)=s*(a(i,jmi)-dot_product(b(i,-mah:jmi-1),b(j,-mah-jmi:-1)))
    809    enddo
