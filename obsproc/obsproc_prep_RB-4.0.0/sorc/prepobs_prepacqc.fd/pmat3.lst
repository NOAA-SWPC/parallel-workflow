    785 !=============================================================================
    786 function fL1Lb_d(m,mah,a,ff)result(b)!                                  [L1Lb]
    787 !=============================================================================
    788 use pietc, only: T,F
    789 use pmat2, only: clipb
    790 integer,                        intent(IN ):: m, mah
    791 real(dp),dimension(m,-mah:mah), intent(IN ):: a
    792 logical,                        intent(out):: ff ! <- failure flag
    793 real(dp),dimension(m,-mah:0)               :: b
    794 integer                                    :: i, j,jmi
    795 real(dp)                                   :: s
    796 !=============================================================================
    797 ff=F
    798 call CLIPB(m,m,mah,0,b)
    799 do j=1,m
    800    s=a(j,0)-dot_product(b(j,-mah:-1),b(j,-mah:-1))
